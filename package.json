{
  "name": "stox",
  "productName": "StoX",
  "version": "1.0.0",
  "description": "StoX GUI - a desktop application for building stox projects",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . stox --platform win32 --arch x64 --out dist/ --overwrite  --asar",
    "installer-win": "node installers/windows/createinstaller.js",
    "package-and-installer-win": "npm run package-win && npm run installer-win",
    "build-frontend": "cd frontend && ng build --prod --base-href && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoXProject/StoX.git"
  },
  "author": "IMR institute (HI)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/StoXProject/StoX/issues"
  },
  "homepage": "https://github.com/StoXProject/StoX#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "electron": "^5.0.6",
    "electron-packager": "^13.1.1",
    "electron-winstaller": "^3.0.4"
  }
}
